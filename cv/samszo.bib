	author = {Ziku, Mariana and Michalakis, Konstantinos and Caridakis, George and Konstadakis, Markos and Trichopoulos, George and Reyes, Everardo and Szoniecky, Samuel and Dupret, Katia and Eschweiler, Jennifer},
	month = jun,
	year = {2023},
