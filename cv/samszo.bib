	keywords = {citizen enhanced open science, community heritage, cultural heritage, European Reform University Alliance, open access, participatory data governance},
	file = {Zenodo Full Text PDF:/home/samszo/snap/zotero-snap/common/Zotero/storage/BUG4AKGA/Ziku et al. - 2023 - ReERUA CoCult Towards a participatory data gover.pdf:application/pdf},
}

